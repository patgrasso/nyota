{
  "name": "nyota",
  "version": "0.0.0",
  "description": "Processes and responds to your words, spoken through the command line.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ignore-pattern coverage",
    "test": "jasmine",
    "coverage": "istanbul cover --include-all-sources -x **/spec/** jasmine",
    "coveralls": "istanbul cover --include-all-sources -x **/spec/** jasmine --captureExceptions && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "nlp",
    "grammar",
    "nyota",
    "cli"
  ],
  "author": "Patrick Grasso",
  "license": "GPL-3",
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "jasmine": "^2.4.1"
  }
}
